# Ignore node_modules
node_modules

# Ignore build/output directories
dist
build
out

# Ignore dependency directories
bower_components

# Ignore coverage reports
coverage

# Ignore log files
*.log

# Ignore environment files
.env
.env.local
.env.*.local

# Ignore lock files (usually not formatted)
package-lock.json
yarn.lock
pnpm-lock.yaml

# Ignore IDE/editor-specific files
.vscode
.idea
*.sublime-project
*.sublime-workspace

# Ignore compiled files
*.min.js
*.bundle.js
*.map
*.dll
*.exe

# Ignore static assets (optional)
*.png
*.jpg
*.jpeg
*.gif
*.svg
*.ico
*.mp4
*.webm

# Ignore markdown files if you don't want Prettier formatting them
# *.md