{
  "name": "familie-ba-minside-mikrofrontend",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "husky",
    "lint": "eslint './src/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint './src/**/*.{js,ts,tsx}' --fix",
    "prettier": "prettier --plugin=prettier-plugin-astro --check './src/**/*.{js,jsx,ts,tsx,json,css,astro}'",
    "prettier:fix": "prettier --plugin=prettier-plugin-astro --write './src/**/*.{js,jsx,ts,tsx,json,css,astro}'",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test": "",
    "validate": "yarn typecheck && yarn lint && yarn prettier"
  },
  "dependencies": {
    "@astrojs/check": "0.9.6",
    "@astrojs/node": "9.5.1",
    "@astrojs/react": "4.4.2",
    "@navikt/aksel-icons": "7.38.0",
    "@navikt/ds-css": "7.38.0",
    "@navikt/ds-react": "7.38.0",
    "@navikt/oasis": "4.1.1",
    "@opentelemetry/sdk-node": "0.209.0",
    "astro": "5.16.4",
    "pino": "10.1.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "rollup-plugin-import-map": "3.0.0"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@types/node": "24.10.1",
    "@types/react": "19.2.1",
    "@types/react-dom": "19.2.1",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "postcss": "8.5.3",
    "postcss-prefix-selector": "2.1.1",
    "prettier": "3.7.4",
    "prettier-plugin-astro": "0.14.1",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.53.0"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc,astro,css,md,html}": [
      "prettier --plugin=prettier-plugin-astro --check './src/**/*.{js,jsx,ts,tsx,json,css,astro}'",
      "eslint --max-warnings=30"
    ]
  }
}
